{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch C++ Docker Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/fast",
            "args": [],
            "targetArchitecture": "x64",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
